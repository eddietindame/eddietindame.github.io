    "refresh": "rm -rf node_modules && rm yarn.lock && yarn",
    "refresh:hard": "rm -rf node_modules && rm yarn.lock && yarn cache clean && yarn",
    "start": "next start",
    "test": "NODE_ENV=test jest"
  },
